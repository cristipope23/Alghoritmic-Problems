#include <iostream>
#include<fstream>
using namespace std;

ifstream fin("plusminus.in");
ofstream fout("plusminus.out");

int n,v[25];

void afisare()
{
    for(int i=1;i<=n;i++)
        cout<<v[i]<<" ";
    cout<<"\n";
}

void back(int k)
{
    if(k>n)
    {
        afisare();
        return;
    }
}

int main()
{
    fin>>n;
    back(1);
    return 0;
}
